      },
      {
        "name": "portalId",
        "type": "String?",
        "notes": "Optional id for the Portal container."
      }
    ]
  },
  "Toast": {
    "title": "Toast",
    "rows": [
      {
        "name": "createToaster.exitMs",
        "type": "int",
        "default": "120",
        "notes": "Presence exit duration for toast animations."
      },
      {
        "name": "createToaster.defaultDurationMs",
        "type": "int",
