  "Menubar": {
    "title": "Menubar",
    "rows": [
      {
        "name": "openKey",
        "type": "String? Function()",
        "required": true,
        "notes": "Key of the currently open top-level menu (or null)."
      },
      {
        "name": "setOpenKey",
        "type": "void Function(String? next)",
        "required": true,
        "notes": "Updates the open top-level menu key."
      },
      {
        "name": "menus",
        "type": "List<MenubarMenu>",
        "required": true,
        "notes": "Defines triggers + menu builders (each menu is built with Menu semantics)."
      },
      {
        "name": "className",
        "type": "String",
        "default": "\"menubar\"",
        "notes": "CSS class for the menubar root."
      },
      {
        "name": "portalId",
        "type": "String?",
        "notes": "Optional id for the Portal container used by all menus."
      }
    ]
  },
  "ContextMenu": {
    "title": "ContextMenu",
    "rows": [
      {
        "name": "open",
        "type": "bool Function()",
        "required": true,
        "notes": "Controls visibility."
      },
      {
        "name": "setOpen",
        "type": "void Function(bool next)",
        "required": true,
        "notes": "Called on dismiss/close; update your signal/state here."
      },
