{
  "Button": {
    "title": "Button",
    "rows": [
      {
        "name": "label",
        "type": "String",
        "required": true,
        "notes": "Visible button label (omit for icon-only buttons)."
      },
      {
        "name": "variant",
        "type": "ButtonVariant",
        "default": "ButtonVariant.normal",
        "notes": "default/secondary/outline/ghost/link/destructive."
      },
      {
        "name": "size",
        "type": "ButtonSize",
        "default": "ButtonSize.normal",
        "notes": "default/sm/lg/icon."
      },
      {
        "name": "disabled",
        "type": "bool",
        "default": "false",
        "notes": "Disables the button."
      },
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Required for icon-only buttons for accessibility."
      },
      {
        "name": "className",
        "type": "String?",
        "notes": "Additional CSS classes (appended)."
      },
      {
        "name": "onClick",
        "type": "void Function(MouseEvent e)?",
        "notes": "Optional click handler."
      }
    ]
  },
  "Badge": {
    "title": "Badge",
    "rows": [
      {
        "name": "label",
        "type": "String",
        "required": true,
        "notes": "Badge text content."
      },
      {
        "name": "variant",
        "type": "BadgeVariant",
        "default": "BadgeVariant.normal",
        "notes": "default/secondary/outline/destructive."
      },
      {
        "name": "className",
        "type": "String?",
        "notes": "Additional CSS classes (appended)."
      }
    ]
  },
  "Separator": {
    "title": "Separator",
    "rows": [
      {
        "name": "orientation",
        "type": "SeparatorOrientation",
        "default": "SeparatorOrientation.horizontal",
        "notes": "horizontal or vertical."
      },
      {
        "name": "decorative",
        "type": "bool",
        "default": "true",
        "notes": "When true, uses role=presentation and aria-hidden=true."
      },
      {
        "name": "className",
        "type": "String",
        "default": "\"separator\"",
        "notes": "CSS class for the root."
      }
    ]
  },
  "Progress": {
    "title": "Progress",
    "rows": [
      {
        "name": "value",
        "type": "double? Function()?",
        "notes": "Current value. Return null to render indeterminate."
      },
      {
        "name": "max",
        "type": "double Function()?",
        "default": "100.0",
        "notes": "Maximum value for determinate progress."
      },
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Accessible label for the progressbar when there's no visible label."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"progress\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "indicatorClassName",
        "type": "String",
        "default": "\"progressIndicator\"",
        "notes": "CSS class for the inner indicator."
      }
    ]
  },
  "Spinner": {
    "title": "Spinner",
    "rows": [
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Accessible label (defaults to \"Loading\")."
      },
      {
        "name": "className",
        "type": "String",
        "default": "\"spinner\"",
        "notes": "CSS class for the root."
      }
    ]
  },
  "Avatar": {
    "title": "Avatar",
    "rows": [
      {
        "name": "src",
        "type": "String? Function()?",
        "notes": "Image src. When omitted/empty or when the image errors, the fallback is shown."
      },
      {
        "name": "alt",
        "type": "String?",
        "notes": "Alt text for the image."
      },
      {
        "name": "fallback",
        "type": "String? Function()?",
        "notes": "Fallback label (typically initials)."
      },
      {
        "name": "size",
        "type": "int",
        "default": "32",
        "notes": "Size in pixels (width/height)."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"avatar\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "imageClassName",
        "type": "String",
        "default": "\"avatarImage\"",
        "notes": "CSS class for the <img>."
      },
      {
        "name": "fallbackClassName",
        "type": "String",
        "default": "\"avatarFallback\"",
        "notes": "CSS class for the fallback."
      }
    ]
  },
  "Breadcrumbs": {
    "title": "Breadcrumbs",
    "rows": [
      {
        "name": "items",
        "type": "Iterable<BreadcrumbItem>",
        "required": true,
        "notes": "Breadcrumb items. The current page should set current=true (or omit href)."
      },
      {
        "name": "ariaLabel",
        "type": "String",
        "default": "\"breadcrumb\"",
        "notes": "Accessible label for the navigation."
      },
      {
        "name": "separatorText",
        "type": "String",
        "default": "\"/\"",
        "notes": "Text used between items (rendered as aria-hidden)."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"breadcrumbs\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "listClassName",
        "type": "String",
        "default": "\"breadcrumbList\"",
        "notes": "CSS class for the list."
      },
      {
        "name": "itemClassName",
        "type": "String",
        "default": "\"breadcrumbItem\"",
        "notes": "CSS class for item <li> nodes."
      },
      {
        "name": "linkClassName",
        "type": "String",
        "default": "\"breadcrumbLink\"",
        "notes": "CSS class for non-current links."
      },
      {
        "name": "pageClassName",
        "type": "String",
        "default": "\"breadcrumbPage\"",
        "notes": "CSS class for the current page span."
      },
      {
        "name": "separatorClassName",
        "type": "String",
        "default": "\"breadcrumbSeparator\"",
        "notes": "CSS class for separator <li> nodes."
      }
    ]
  },
  "Alert": {
    "title": "Alert",
    "rows": [
      {
        "name": "children",
        "type": "Iterable<Node>",
        "required": true,
        "notes": "Alert contents (commonly AlertTitle + AlertDescription)."
      },
      {
        "name": "variant",
        "type": "AlertVariant",
        "default": "AlertVariant.normal",
        "notes": "default/destructive."
      },
      {
        "name": "className",
        "type": "String?",
        "notes": "Additional CSS classes (appended)."
      },
      {
        "name": "role",
        "type": "String",
        "default": "\"alert\"",
        "notes": "ARIA role (\"alert\" by default). Set to \"\" to omit."
      }
    ]
  },
  "NavigationMenu": {
    "title": "NavigationMenu",
    "rows": [
      {
        "name": "items",
        "type": "Iterable<NavigationMenuItem>",
        "required": true,
        "notes": "Menu items: each provides a trigger element + a content panel element."
      },
      {
        "name": "openOnHover",
        "type": "bool",
        "default": "true",
        "notes": "Whether hover switches/opens panels."
      },
      {
        "name": "closeDelayMs",
        "type": "int",
        "default": "140",
        "notes": "Delay before closing when leaving trigger/panel."
      },
      {
        "name": "openDelayMs",
        "type": "int",
        "default": "0",
        "notes": "Delay before opening on hover."
      },
      {
        "name": "ariaLabel",
        "type": "String",
        "default": "\"navigation\"",
        "notes": "Accessible label for the navigation region."
      },
      {
        "name": "id",
        "type": "String?",
        "notes": "Optional root id."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"navigationMenu\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "listClassName",
        "type": "String",
        "default": "\"navigationMenuList\"",
        "notes": "CSS class for the trigger list."
      },
      {
        "name": "triggerClassName",
        "type": "String",
        "default": "\"navigationMenuTrigger\"",
        "notes": "CSS class applied to triggers."
      },
      {
        "name": "contentClassName",
        "type": "String",
        "default": "\"navigationMenuContent\"",
        "notes": "CSS class for the popover panel."
      }
    ]
  },
  "Checkbox": {
    "title": "Checkbox",
    "rows": [
      {
        "name": "checked",
        "type": "bool Function()",
        "required": true,
        "notes": "Controlled checked state."
      },
      {
        "name": "setChecked",
        "type": "void Function(bool next)",
        "required": true,
        "notes": "Called when the checkbox toggles."
      },
      {
        "name": "indeterminate",
        "type": "bool Function()?",
        "notes": "Optional indeterminate (mixed) state; sets aria-checked=\"mixed\"."
      },
      {
        "name": "setIndeterminate",
        "type": "void Function(bool next)?",
        "notes": "Optional setter for indeterminate. When indeterminate is true and the user toggles, we clear indeterminate and set checked=true."
      },
      {
        "name": "disabled",
        "type": "bool Function()?",
        "notes": "Disables interactions and sets aria-disabled/data-disabled."
      },
      {
        "name": "id",
        "type": "String?",
        "notes": "Optional element id."
      },
      {
        "name": "className",
        "type": "String",
        "default": "\"checkbox\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Accessible label for icon-only usage. If you render visible text, prefer linking via a <label> element."
      }
    ]
  },
  "RadioGroup": {
    "title": "RadioGroup",
    "rows": [
      {
        "name": "items",
        "type": "Iterable<RadioGroupItem>",
        "required": true,
        "notes": "Items to render. Each item provides a key + an interactive element (typically a <button>)."
      },
      {
        "name": "value",
        "type": "String? Function()",
        "required": true,
        "notes": "Controlled selected key."
      },
      {
        "name": "setValue",
        "type": "void Function(String next)",
        "required": true,
        "notes": "Called when selection changes (click/Space/Enter/Arrow keys)."
      },
      {
        "name": "orientation",
        "type": "Orientation Function()?",
        "notes": "Keyboard orientation: vertical (default) or horizontal."
      },
      {
        "name": "shouldFocusWrap",
        "type": "bool Function()?",
        "notes": "Whether Arrow navigation wraps from last↔first."
      },
      {
        "name": "disabled",
        "type": "bool Function()?",
        "notes": "Disables the whole group (all items)."
      },
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Accessible label for the radiogroup when there is no visible group label."
      },
      {
        "name": "id",
        "type": "String?",
        "notes": "Optional root id."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"radioGroup\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "itemClassName",
        "type": "String",
        "default": "\"radioItem\"",
        "notes": "CSS class added to each item element."
      }
    ]
  },
  "ToggleGroup": {
    "title": "ToggleGroup",
    "rows": [
      {
        "name": "type",
        "type": "ToggleGroupType Function()",
        "required": true,
        "notes": "Single or multiple selection."
      },
      {
        "name": "items",
        "type": "Iterable<ToggleGroupItem>",
        "required": true,
        "notes": "Items to render. Each item provides a key + an interactive element (typically a <button>)."
      },
      {
        "name": "value",
        "type": "String? Function()?",
        "notes": "Single mode only: controlled selected key (nullable for deselect)."
      },
      {
        "name": "setValue",
        "type": "void Function(String? next)?",
        "notes": "Single mode only: called on toggles/activation."
      },
      {
        "name": "values",
        "type": "Set<String> Function()?",
        "notes": "Multiple mode only: controlled set of pressed keys."
      },
      {
        "name": "setValues",
        "type": "void Function(Set<String> next)?",
        "notes": "Multiple mode only: called on toggles/activation."
      },
      {
        "name": "disallowEmptySelection",
        "type": "bool Function()?",
        "notes": "Prevents clearing the last pressed item."
      },
      {
        "name": "disabled",
        "type": "bool Function()?",
        "notes": "Disables the whole group (all items)."
      },
      {
        "name": "orientation",
        "type": "Orientation Function()?",
        "notes": "Keyboard orientation: horizontal (default) or vertical."
      },
      {
        "name": "shouldFocusWrap",
        "type": "bool Function()?",
        "notes": "Whether Arrow navigation wraps from last↔first."
      },
      {
        "name": "ariaLabel",
        "type": "String?",
        "notes": "Accessible label for the group when there is no visible label."
      },
      {
        "name": "id",
        "type": "String?",
        "notes": "Optional root id."
      },
      {
        "name": "rootClassName",
        "type": "String",
        "default": "\"toggleGroup\"",
        "notes": "CSS class for the root."
      },
      {
        "name": "itemClassName",
        "type": "String",
        "default": "\"toggleItem\"",
        "notes": "CSS class added to each item element."
      }
    ]
  },
  "Dialog": {
    "title": "Dialog",
    "rows": [
      {
        "name": "open",
        "type": "bool Function()",
        "required": true,
        "notes": "Controls visibility."
      },
      {
        "name": "setOpen",
        "type": "void Function(bool next)",
        "required": true,
        "notes": "Called on dismiss/close; update your signal/state here."
      },
      {
        "name": "builder",
        "type": "DialogBuilder (close) → HTMLElement",
        "required": true,
        "notes": "Builds the dialog content; call close(reason?) to dismiss."
      },
      {
        "name": "onClose",
        "type": "void Function(String reason)?",
        "notes": "Dismiss reason: escape/outside/close/etc (implementation-defined)."
      },
      {
        "name": "modal",
        "type": "bool",
        "default": "true",
        "notes": "When true: traps focus, disables outside pointer events, scroll-locks, and aria-hides outside content."
      },
      {
        "name": "backdrop",
        "type": "bool",
        "default": "false",
        "notes": "Renders a backdrop element behind the dialog panel."
      },
      {
        "name": "exitMs",
        "type": "int",
        "default": "120",
        "notes": "Presence exit duration for animations."
      },
      {
        "name": "initialFocus",
        "type": "HTMLElement?",
        "notes": "Element to focus on open (modal focus-scope only)."
      },
      {
        "name": "restoreFocus",
        "type": "bool",
        "default": "true",
        "notes": "Restore focus to the previously focused element on close."
      },
      {
        "name": "onOpenAutoFocus",
        "type": "void Function(FocusScopeAutoFocusEvent event)?",
        "notes": "Preventable hook to customize autofocus on mount."
      },
      {
        "name": "onCloseAutoFocus",
        "type": "void Function(FocusScopeAutoFocusEvent event)?",
        "notes": "Preventable hook to customize autofocus on unmount."
      },
      {
        "name": "labelledBy",
        "type": "String?",
        "notes": "Sets aria-labelledby on the dialog content."
      },
      {
        "name": "describedBy",
        "type": "String?",
        "notes": "Sets aria-describedby on the dialog content."
      },
      {
        "name": "role",
        "type": "String",
        "default": "\"dialog\"",
        "notes": "Use \"alertdialog\" for urgent confirmations."
      },
      {
        "name": "portalId",
        "type": "String?",
        "notes": "Optional id for the Portal container."
      }
    ]
  },
  "Popover": {
    "title": "Popover",
    "rows": [
      {
        "name": "open",
        "type": "bool Function()",
        "required": true,
        "notes": "Controls visibility."
      },
      {
        "name": "setOpen",
        "type": "void Function(bool next)",
        "required": true,
        "notes": "Called on dismiss/close; update your signal/state here."
      },
      {
        "name": "anchor",
        "type": "Element?",
        "notes": "Anchor element used for Popper positioning (recommended)."
      },
      {
        "name": "builder",
        "type": "PopoverBuilder (close) → HTMLElement",
        "required": true,
        "notes": "Builds the panel element; call close(reason?) to dismiss."
      },
      {
        "name": "onClose",
        "type": "void Function(String reason)?",
        "notes": "Dismiss reason: escape/outside/close/etc."
      },
      {
        "name": "trapFocus",
        "type": "bool",
        "default": "false",
        "notes": "When true, traps focus inside the panel (Popover becomes modal-like)."
      },
      {
        "name": "initialFocus",
        "type": "HTMLElement?",
        "notes": "Element to focus on open (when trapFocus or autoFocus is used)."
      },
      {
        "name": "placement",
        "type": "String",
        "default": "\"bottom-start\"",
        "notes": "Floating UI placement."
      },
      {
        "name": "offset",
        "type": "double",
        "default": "8",
        "notes": "Distance between anchor and panel."
      },
      {
        "name": "viewportPadding",
        "type": "double",
        "default": "8",
        "notes": "Padding when clamping/shift/flip near viewport edges."
      },
      {
        "name": "flip",
        "type": "bool",
        "default": "true",
        "notes": "Allow flipping to remain visible."
      },
      {
        "name": "slide",
        "type": "bool",
        "default": "true",
        "notes": "Keep popover within viewport by sliding along the anchor."
      },
      {
        "name": "overlap",
        "type": "bool",
        "default": "false",
        "notes": "Allow overlap with the anchor when shifting."
      },
      {
        "name": "hideWhenDetached",
        "type": "bool",
        "default": "false",
        "notes": "Hide the panel when the anchor is detached/offscreen."
      },
      {
        "name": "exitMs",
        "type": "int",
        "default": "120",
        "notes": "Presence exit duration for animations."
      },
      {
        "name": "portalId",
        "type": "String?",
        "notes": "Optional id for the Portal container."
      }
    ]
  },
  "Tooltip": {
    "title": "Tooltip",
    "rows": [
      {
        "name": "open",
        "type": "bool Function()",
        "required": true,
        "notes": "Controls visibility."
      },
      {
        "name": "setOpen",
        "type": "void Function(bool next)",
        "required": true,
        "notes": "Called by hover/focus interactions; update your signal/state here."
      },
      {
        "name": "trigger",
        "type": "Element",
        "required": true,
        "notes": "The element that shows the tooltip on hover/focus."
      },
      {
        "name": "builder",
        "type": "TooltipBuilder (close) → HTMLElement",
        "required": true,
        "notes": "Builds the tooltip element."
      },
      {
        "name": "openDelayMs",
        "type": "int",
        "default": "500",
        "notes": "Delay before opening on hover."
      },
      {
        "name": "closeDelayMs",
        "type": "int",
        "default": "150",
        "notes": "Delay before closing on pointer leave."
      },
      {
        "name": "interactive",
        "type": "bool",
        "default": "false",
        "notes": "When true, tooltip can receive pointer events (useful for richer content)."
      },
      {
        "name": "placement",
        "type": "String",
        "default": "\"top\"",
        "notes": "Floating UI placement."
      },
      {
        "name": "portalId",
        "type": "String?",
        "notes": "Optional id for the Portal container."
      }
    ]
  },
  "DropdownMenu": {
    "title": "DropdownMenu",
    "rows": [
      {
        "name": "open",
        "type": "bool Function()",
        "required": true,
        "notes": "Controls visibility."
      },
      {
        "name": "setOpen",
        "type": "void Function(bool next)",
