{
  "name": "dart-web-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "provision:dart": "bash scripts/provision-dart.sh",
    "dart:get": "node scripts/dart-pub-get.mjs",
    "predev": "npm run dart:get",
    "dev": "vite",
    "prebuild": "npm run dart:get",
    "build": "vite build",
    "preview": "vite preview",
    "debug:ui": "node scripts/debug-ui.mjs",
    "debug:ui:dev": "node scripts/debug-ui.mjs --mode dev",
    "debug:ui:preview": "node scripts/debug-ui.mjs --mode preview",
    "debug:ui:ci": "node scripts/debug-ui.mjs --mode preview --timeout-ms 240000",
    "debug:solid-dom": "node scripts/debug-ui.mjs --mode dev --path \"/?solid=1\" --scenario solid-dom --expect-h1 \"Solid DOM Demo\" --expect-selector \"#solid-root\"",
    "debug:solid-dom:ci": "node scripts/debug-ui.mjs --mode dev --timeout-ms 240000 --path \"/?solid=1\" --scenario solid-dom --expect-h1 \"Solid DOM Demo\" --expect-selector \"#solid-root\""
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "devDependencies": {
    "playwright": "^1.57.0",
    "vite": "^7.3.0",
    "vite-plugin-dart": "^1.0.3"
  },
  "dependencies": {
    "morphdom": "^2.7.7"
  }
}
